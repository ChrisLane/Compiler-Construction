// This is the iterative bisection algorithm. This should parse.
bisection (f, a, b, tol, nmax) {
    var n = 1;

    while (n <= nmax) {
        var c = a + b;
        c = c / 2;

        if (f(c) == 0 || (b - a) / 2 <= tol) {
            return $c;
        } else {};

        n = n + 1;

        if (f(c) == f(a)) {
            var a = c;
        } else {
            var b = c;
        };
    };
}
